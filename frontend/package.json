{
  "name": "ridecare-devcloud",
  "version": "0.14.0",
  "scripts": {
    "build:dev": "ng build --configuration=dev && ng analytics off",
    "build:qa": "ng build --configuration=qa && ng analytics off",
    "build:local": "ng build --configuration=local && ng analytics off",
    "build": "ng build && ng analytics off",
    "doc:app": "compodoc -p tsconfig.app.json -t --hideGenerator -d \"dist/docs/app\"",
    "lint": "ng lint",
    "lint:app": "ng lint ridecare-devcloud",
    "ng": "ng",
    "postbuild:app": "npm run doc:app",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "pree2e": "webdriver-manager update --ignore_ssl=true --gecko false",
    "sonar": "sonar-scanner",
    "start:dev": "ng serve --host=127.0.0.1 --configuration=dev && ng analytics off",
    "start:qa": "ng serve --host=127.0.0.1 --configuration=qa && ng analytics off",
    "start:local": "ng serve --host=127.0.0.1 --configuration=local && ng analytics off",
    "start:subpath": "ng serve --host=127.0.0.1 --port 4202 --serve-path=/test/ --base-href=/test/",
    "test": "ng test",
    "test:app": "ng test ridecare-devcloud --browsers=Chrome",
    "test-headless": "npm run test:headless:app",
    "test:headless:app": "ng test --code-coverage --watch=false --browsers=ChromeHeadlessNoSandbox",
    "server": "ts-node ./server/server.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/platform-browser-dynamic": "^14.2.6",
    "@azure/msal-angular": "^2.5.0",
    "@azure/msal-browser": "^2.32.0",
    "@babel/core": "^7.13.0",
    "@bci-web-core/common-styles": "^4.3.0",
    "@bci-web-core/core": "^14.0.3",
    "@bci-web-core/material-theme": "^14.0.3",
    "@bci-web-core/web-components": "^6.1.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@schematics/angular": "^14.2.6",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.9.1",
    "chartjs-adapter-date-fns": "^2.0.0",
    "chartjs-plugin-zoom": "^1.2.0",
    "classlist.js": "^1.1.20150312",
    "date-fns": "^2.29.3"
  },
  "overrides": {
    "@angular/cdk": "~14.2.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.6",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "^14.2.6",
    "@angular/compiler-cli": "^14.2.6",
    "@compodoc/compodoc": "^1.1.18",
    "@types/chart.js": "^2.9.37",
    "@types/jasmine": "^3.10.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^18.11.15",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "5.46.1",
    "@typescript-eslint/parser": "5.37.0",
    "cpx": "^1.5.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jasmine-core": "^4.5.0",
    "jasmine-reporters": "^2.5.2",
    "jasmine-spec-reporter": "^7.0.0",
    "json-server": "^0.17.0",
    "karma": "^6.3.11",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.1.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "sonar-scanner": "^3.1.0",
    "ts-node": "^10.4.0",
    "puppeteer": "^8.0.0"
  }
}
