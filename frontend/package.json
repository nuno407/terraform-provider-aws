{
  "name": "ridecare-operator-webportal",
  "version": "0.14.0",
  "scripts": {
    "build:dev": "ng build --configuration=dev && ng analytics off",
    "build:qa": "ng build --configuration=qa && ng analytics off",
    "build:local": "ng build --configuration=local && ng analytics off",
    "build": "ng build && ng analytics off",
    "doc:app": "compodoc -p tsconfig.app.json -t --hideGenerator -d \"dist/docs/app\"",
    "e2e": "ng e2e --webdriver-update=false",
    "e2e-headless": "ng e2e --webdriver-update=false --protractor-config=./e2e/protractor-ci.conf.js",
    "lint": "ng lint",
    "lint:app": "ng lint ridecare-operator-webportal",
    "ng": "ng",
    "postbuild:app": "npm run doc:app",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "pree2e": "webdriver-manager update --ignore_ssl=true --gecko false",
    "sonar": "sonar-scanner",
    "start:dev": "ng serve --configuration=dev && ng analytics off",
    "start:qa": "ng serve --configuration=qa && ng analytics off",
    "start:local": "ng serve --configuration=local && ng analytics off",
    "start:subpath": "ng serve --port 4202 --serve-path=/test/ --base-href=/test/",
    "test": "ng test",
    "test:app": "ng test ridecare-operator-webportal --browsers=Chrome",
    "test-headless": "npm run test:headless:app",
    "test:headless:app": "ng test --code-coverage --watch=false --browsers=ChromeHeadlessNoSandbox",
    "server": "ts-node ./server/server.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^12.2.13",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "^12.2.13",
    "@angular/compiler": "^12.2.13",
    "@angular/core": "^12.2.13",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "^12.2.13",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "^12.2.13",
    "@angular/platform-browser-dynamic": "^12.2.13",
    "@angular/router": "^12.2.13",
    "@aws-amplify/ui-angular": "^1.0.7",
    "@babel/core": "^7.13.0",
    "@bci-web-core/common-styles": "^2.10.0",
    "@bci-web-core/core": "^11.4.0",
    "@bci-web-core/material-theme": "^11.0.0",
    "@bci-web-core/web-components": "^2.3.0",
    "@juggle/resize-observer": "^3.3.1",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "angularx-qrcode": "^12.0.3",
    "aws-amplify": "^4.3.12",
    "aws-sdk": "^2.1056.0",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.7.0",
    "chartjs-adapter-date-fns": "^2.0.0",
    "classlist.js": "^1.1.20150312",
    "date-fns": "^2.28.0",
    "hammerjs": "^2.0.8",
    "moment": "^2.29.1",
    "ng2-charts": "^3.0.8",
    "puppeteer": "^8.0.0",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^12.2.13",
    "@angular/cli": "^12.2.15",
    "@angular/compiler-cli": "^12.2.13",
    "@compodoc/compodoc": "^1.1.18",
    "@types/chart.js": "^2.9.35",
    "@types/jasmine": "^3.10.3",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^17.0.8",
    "@types/webpack-env": "^1.16.3",
    "chartjs-plugin-zoom": "^1.2.0",
    "codelyzer": "^6.0.2",
    "cpx": "^1.5.0",
    "jasmine-core": "^4.0.0",
    "jasmine-reporters": "^2.5.0",
    "jasmine-spec-reporter": "^7.0.0",
    "json-server": "^0.17.0",
    "karma": "^6.3.11",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-coverage": "^2.1.0",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "karma-junit-reporter": "^2.0.1",
    "prettier": "^2.5.1",
    "protractor": "^7.0.0",
    "protractor-console-plugin": "^0.1.1",
    "sonar-scanner": "^3.1.0",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "~4.3.5"
  }
}
